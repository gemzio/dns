{
    "name": "gemz/dns",
    "description": "Get DNS records",
    "keywords": [
        "gemz",
        "dns"
    ],
    "homepage": "https://github.com/gemzio/dns",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Riehl",
            "email": "stefan@sriehl.com",
            "homepage": "https://gemz.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "react/dns": "^1.2",
        "react/event-loop": "^1.1.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3|^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2",
        "phpstan/phpstan": "^0.12.8"
    },
    "autoload": {
        "psr-4": {
            "Gemz\\Dns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gemz\\Dns\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
